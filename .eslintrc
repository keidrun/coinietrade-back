{
  "extends": ["eslint:recommended", "prettier"],
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "all",
        "singleQuote": true,
        "semi": true,
        "printWidth": 80
      }
    ]
  },
  "plugins": ["prettier"]
}
